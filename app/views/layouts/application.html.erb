    <main id="mainContainer" class="uk-flex uk-flex-column">
    <!--note: column to get alerts to show, but breaks the bike show page (it needs to be rebuilt anyways)-->
      
      <div class="uk-container uk-text-center uk-margin-top">
      <!-- Displays flash message on each page -->
      <% flash.each do |key, value| %>
        <!-- If the key is the :errors symbol, then display all the messages -->
        <!-- that were stored in the hash -->
        <% if key == "errors" %>
          <div class="uk-alert uk-alert-danger">
          <% value.each do |k, v| %>
            <p><%= k.capitalize %>
            <!-- Stop at length-1 because we only want 'and' between messages -->
            <% for i in 0...(v.length-1) %>
              <%= v[i] %> and
            <% end %>
            <!-- Display the final error message -->
            <%= v[v.length-1] %></p>
          <% end %>
          </div>
        <% else %>
          <div class="uk-alert uk-alert-<%= key %>"><%= value %></div>
        <% end %>
      <% end %>
      </div>
	  
