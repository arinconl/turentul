<% @pageName = "#{@renter.renterName.split(" ")[0]}" %>
<% @pageSection = "Profile" %>

<% if session[:logged_in] %>
    <%= render :partial => '/renters/renter_show' %>
<% else %>
    <%= render :partial => 'login_section' %>
<% end %>

<% if current_user.id==params[:id] %>
		<%= render :partial => '/renters/renter_tickets', :locals => {:tickets => @tickets} %>
<% end %>
